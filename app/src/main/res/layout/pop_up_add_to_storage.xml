<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/blue_0"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/dialog_delete"
        android:layout_width="360dp"
        android:layout_height="350dp"
        android:background="#80C6D9FF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="190dp">


        <TextView
            android:id="@+id/rejestracja"
            android:layout_width="320dp"
            android:layout_height="133dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:fontFamily="@font/quicksand_medium"

            android:gravity="center"

            android:text="@string/add_to_storage"
            android:textAppearance="@style/rejestracja"
            android:textColor="@color/black_gray"
            tools:layout_editor_absoluteX="20dp"
            tools:layout_editor_absoluteY="16dp" />

        <Button
            android:id="@+id/button_yes"
            android:layout_width="236dp"
            android:layout_height="70dp"

            android:layout_alignParentLeft="true"

            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:backgroundTint="@color/blue_1"

            android:clickable="true"
            android:drawableLeft="@drawable/done_24_50"

            android:fontFamily="@font/quicksand_medium"

            android:text="@string/tak"

            android:textAllCaps="false"
            android:textColor="@color/black_gray"

            android:textSize="16dp"
            app:strokeColor="@color/blue_2"
            app:strokeWidth="1dp"
            tools:layout_editor_absoluteX="62dp"
            tools:layout_editor_absoluteY="167dp" />

        <Button
            android:id="@+id/button_no"
            android:layout_width="236dp"
            android:layout_height="70dp"

            android:layout_alignParentLeft="true"

            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:backgroundTint="@color/blue_1"

            android:clickable="true"
            android:drawableLeft="@drawable/close_24_50"

            android:fontFamily="@font/quicksand_medium"

            android:text="@string/nie"

            android:textAllCaps="false"
            android:textColor="@color/black_gray"
            android:textSize="16dp"

            app:strokeColor="@color/blue_2"
            app:strokeWidth="1dp"

            tools:layout_editor_absoluteX="62dp"
            tools:layout_editor_absoluteY="247dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>